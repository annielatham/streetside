<div class="page-header">
  <h3>Visit #<%= @visit.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Vehicle</dt>
      <dd><%= @visit.vehicle_id %></dd>

      <dt>Permit</dt>
      <dd><%= @visit.permit_id %></dd>

      <dt>Start time</dt>
      <dd><%= @visit.start_time %></dd>

      <dt>End time</dt>
      <dd><%= @visit.end_time %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/visits" class="btn btn-primary">
        Back
      </a>
      <a href="/visits/<%= @visit.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_visit/<%= @visit.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
